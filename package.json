{
  "name": "diabetics-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf dist && mkdir dist && mkdir dist/public && mkdir dist/server",
    "copy-public": "cp -r public/* dist/public/ ",
    "build-server": "./node_modules/.bin/babel -d dist/server src/server -s",
    "build-ui": "webpack -p",
    "build": "npm run clean && npm run build-server && npm run build-ui && npm run copy-public",
    "postinstall": "npm run build",
    "start": "node ./bin/www",
    "dev": "npm run build && npm start"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.6",
    "body-parser": "~1.15.1",
    "chart.js": "^2.2.2",
    "co": "^4.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "fhir.js": "0.0.19",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "mongoose": "^4.6.0",
    "morgan": "~1.7.0",
    "querystring": "0.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "~2.3.0",
    "thunk": "0.0.1",
    "webpack": "^1.13.2",
    "rc-table": "*",
    "css-loader": "^0.25.0",
    "style-loader": "^0.13.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {}
}